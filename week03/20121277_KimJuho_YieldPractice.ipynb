    "%%writefile Fibonacci1.py\n",
    "\n",
    "def Fibo_func(N):\n",
    "    yield 0\n",
    "    a, b = 0, 1\n",
    "    for _ in range(N):\n",
    "        a, b = b, a+b\n",
    "        yield a"
   ]
